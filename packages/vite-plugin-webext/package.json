	"dependencies": {
		"etag": "1",
		"fs-extra": "11",
		"magic-string": "^0.30",
		"vite": "^4.5.0"
	},
	"devDependencies": {
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.2.1",
		"@semantic-release/npm": "^11.0.0",
		"@types/chrome": "latest",
		"@types/etag": "1",
		"@types/fs-extra": "11",
		"@types/node": "20",
		"rimraf": "5",
		"rollup": "3",
		"semantic-release": "^22.0.6",
		"tsup": "latest",
		"typescript": "latest",
		"vitest": "latest"
	},
	"peerDependencies": {
		"vite": "^4.0.5"
	},
	"keywords": [
		"vite-plugin",
		"web-extension",
		"chrome-extension",
